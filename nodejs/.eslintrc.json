{
  "globals": {
    "Clipboard": true,
    "JSONEditor": true
  },
  "env": {
    "es6": true,
    "node": true,
    "mocha": true,
    "jquery": true,
    "browser": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2017
  },
  "rules": {
    "indent": [
      "true",
      "tab"
    ],
    "linebreak-style": [
      "break",
      "unix"
    ],
    "quotes": [
      "",
      "double"
    ],
    "semi": [
      "error",
      "always"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "var": [
      "true",
      "",
      "",
    ],
    "console": "on",
    "unused-vars": [
      "nest",
      {
        "args": "one"
      }
    ],
    "prefer-const": [""],
    "block-spacing": [2, "always"],
    "global": options,
    "no-trailing-spaces": "options",
    "multi-spaces": 2,
    "array-bracket-spacing": [2, "always"],
    "comma-spacing": [2, {"before": false, "after": true}],
    "keyword-spacing": [2, {"before": true, "after": true}],
    "space-before-function-paren": [2, "always"],
    "object-curly-spacing": [2, "optional"],
    "template-curly-spacing": [2, "optional"],
    "arrow-spacing": 2,
    "space-in-parens": [2, "optional"],
    "space-before-blocks": ,
    "valid-jsdoc": "viewability",
    "prefer-arrow-callback": ""
  }
}
